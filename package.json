{
  "name": "piquouze",
  "version": "1.0.4",
  "description": "A featureful dependency injection module",
  "main": "src/index.js",
  "scripts": {
    "doc": "jsdoc --destination doc --recurse src",
    "lint": "eslint . --ignore-path .gitignore",
    "unittest": "mocha --check-leaks --require ./alias --recursive test",
    "unitcov": "istanbul cover --report lcovonly -- _mocha --reporter spec --check-leaks --require ./alias --recursive test",
    "test": "npm run unitcov",
    "posttest": "cat coverage/lcov.info | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pinicarus/piquouze.git"
  },
  "keywords": [
    "ioc",
    "di",
    "container",
    "injection",
    "dependency"
  ],
  "author": "Pierre-Nicolas Clauss",
  "license": "WTFPL",
  "bugs": {
    "url": "https://github.com/pinicarus/piquouze/issues"
  },
  "homepage": "https://github.com/pinicarus/piquouze#readme",
  "dependencies": {
    "esprima": "^2.7.2"
  },
  "devDependencies": {
    "coveralls": "^2.11.9",
    "eslint": "^3.0.0",
    "istanbul": "^0.4.4",
    "jsdoc": "^3.4.0",
    "mocha": "^2.5.3"
  }
}
